    <div class="col-md-4">
        <label for="clt_lien_obs" class="font-weight-bold mt-3">Observation</label>
        <input type="text" name="clt_lien_obs" id="clt_lien_obs" class="form-control bg-white  font-weight-bold"  value="{{ dataUrgence.observation }}">
    </div>
</div>