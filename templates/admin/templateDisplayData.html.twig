
{% for dataToImport in dataToImports %}
    <div class="border rounded parent_bloc_elem mt-3 p-3">
        <button class="btn btn-outline-danger btn-sm btn_close_elem"><i class="fa fa-times"></i></button>
        <h5 class="title_form mt-3 mb-2 text-black text-uppercase">Produits {{dataToImport[1]}}</h5>
        <div class="w-100">
            <ul class="list-unstyled list-inline">
                <li class="text-success mx-4 list-inline-item text-uppercase"><span class="font-weight-bold">Catégorie</span> : {{dataToImport[0]}}</li>
                <li class="text-success mx-4 list-inline-item text-uppercase"><span class="font-weight-bold">Code Produit</span> : {{dataToImport[1]}}</li>
                <li class="text-success mx-4 list-inline-item text-uppercase"><span class="font-weight-bold">Unité</span> : {{dataToImport[2]}}</li>
                <li class="text-success mx-4 list-inline-item text-uppercase"><span class="font-weight-bold">Nom</span> : {{dataToImport[3]}}</li>
                <li class="text-success mx-4 list-inline-item text-uppercase"><span class="font-weight-bold">Désignation</span> : {{dataToImport[4]}}</li>
            </ul>
        </div>
        <h5 class="title_form mt-2 mb-2 text-black text-uppercase">Variation de Produit</h5>
        <div class="table-responsive">
            <table class="table table-sm table-bordered">
                <thead class="thead-dark">
                    <tr>
                        <th>Code Produit</th>
                        <th>Indice</th>
                        <th>Entrepôt</th>
                        <th>Fournisseur</th>
                        <th>Prix Achat</th>
                        <th>Charge</th>
                        <th>Prix revient</th>
                        <th>Marge</th>
                        <th>Stock</th>
                        <th>Stock Alert</th>
                        <th>Prix de vente</th>
                        <th>Expirée le</th>
                    </tr>
                </thead>  
                <tbody>
                    {% for variation in dataToImport.variation %}
                        <tr>
                            <td>{{variation[0]}}</td>
                            <td>{{variation[1]}}</td>
                            <td>{{variation[2]}}</td>
                            <td>{{variation[3]}}</td>
                            <td>{{variation[5]}}</td>
                            <td>{{variation[6]}}</td>
                            <td>{{variation[7]}}</td>
                            <td>{{variation[8]}}</td>
                            <td>{{variation[9]}}</td>
                            <td>{{variation[10]}}</td>
                            <td>{{variation[11]}}</td>
                            <td>{{variation[4]}}</td>
                        </tr>
                    {% endfor %}
                </tbody>  
            </table>
        </div>
    </div>
{% endfor %}